# 1. Primeiro, vamos organizar suas imagens na estrutura correta
def organize_images(source_dir, target_dir, class_name):
    os.makedirs(os.path.join(target_dir, class_name), exist_ok=True) #Cria a pasta de destino (se n√£o existir):

    for img_file in os.listdir(source_dir): #Percorre todos os arquivos na pasta original:
        if img_file.lower().endswith(('.png', '.jpg', '.jpeg', '.webp', '.avif')): #Verifica se √© imagem
            src = os.path.join(source_dir, img_file) # Caminho original
            dst = os.path.join(target_dir, class_name, img_file) # Caminho novo
            shutil.copy(src, dst) # Copia (n√£o move)

# Caminhos (ajuste conforme seu ambiente)
source_dir_brancas = '/content/drive/MyDrive/Projeto ML/Projeto ML/Dataset-img/mulher-branca'
source_dir_pretas = '/content/drive/MyDrive/Projeto ML/Projeto ML/Dataset-img/mulher-preta'
target_dir = '/content/dataset_organizado'

# Organizando as imagens
"""
source_dir_brancas

O que √©?
√â uma vari√°vel que armazena o caminho (path) do diret√≥rio onde est√£o armazenadas as imagens originais de mulheres brancas ou pretas.

os.path.join()

O que √©?
√â uma fun√ß√£o do m√≥dulo os que combina partes de um caminho de forma correta para qualquer sistema operacional.

target_dir

O que √©?
√â a vari√°vel que armazena o caminho do diret√≥rio principal onde as imagens organizadas ser√£o armazenadas.
"""
organize_images(source_dir_brancas, os.path.join(target_dir, 'treinamento'), 'brancas')
organize_images(source_dir_pretas, os.path.join(target_dir, 'treinamento'), 'pretas')

# 2. Agora o treinamento
IMG_SIZE = (224, 224)
BATCH_SIZE = 8  # Reduzi porque tem poucas imagens
EPOCHS = 15 # Uma passagem completa por todo o conjunto de treinamento.

# Data augmentation
train_datagen = ImageDataGenerator(
    rescale=1./255, # Normaliza os valores dos pixels para [0,1]
    validation_split=0.2, # Reserva 20% dos dados para valida√ß√£o
    rotation_range=20, # Rotaciona imagens aleatoriamente at√© 20 graus
    width_shift_range=0.2, # Desloca imagem horizontalmente (20% da largura)
    height_shift_range=0.2, # Desloca imagem verticalmente (20% da altura)
    horizontal_flip=True # Inverte imagem horizontalmente aleatoriamente
)

train_generator = train_datagen.flow_from_directory(
    os.path.join(target_dir, 'treinamento'), # Pasta com imagens
    target_size=IMG_SIZE,   # Redimensiona imagens
    batch_size=BATCH_SIZE,  # N√∫mero de imagens por lote
    class_mode='binary',  # Classifica√ß√£o bin√°ria
    subset='training' # Usa parte de treino (80%)
)

validation_generator = train_datagen.flow_from_directory(
    os.path.join(target_dir, 'treinamento'),
    target_size=IMG_SIZE,
    batch_size=BATCH_SIZE,
    class_mode='binary',
    subset='validation'  # Usa parte de valida√ß√£o (20%)
)

# Modelo simples
# Defini√ß√£o da arquitetura do modelo
model = Sequential([
    # Primeira camada convolucional
    Conv2D(32, (3, 3), activation='relu', input_shape=(IMG_SIZE[0], IMG_SIZE[1], 3)),
    MaxPooling2D(2, 2), # Redu√ß√£o de dimensionalidade
    Conv2D(64, (3, 3), activation='relu'),  # Segunda camada convolucional
    MaxPooling2D(2, 2),
    Flatten(), # Prepara√ß√£o para camadas densas(Transforma matriz em vetor)
    Dense(128, activation='relu'), # Camada densa (fully connected)
    Dense(1, activation='sigmoid') # Camada de sa√≠da (classifica√ß√£o bin√°ria)
])

# Configura√ß√£o do processo de treinamento
model.compile(
    optimizer='adam',  # Otimizador eficiente
    loss='binary_crossentropy', # Fun√ß√£o de perda para classifica√ß√£o bin√°ria
    metrics=['accuracy'] # M√©trica a ser monitorada
)

# Treinamento
history = model.fit(
    train_generator, # Dados de treino
    validation_data=validation_generator, # Dados de valida√ß√£o
    epochs=EPOCHS # N√∫mero de √©pocas
)


# Salvar modelo
model.save('modelo_classificacao_racial.h5')
